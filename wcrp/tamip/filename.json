{
    "identifier_type": "filename",
    "scope": "wcrp:tamip",
    "seperator": "_",
    "template": "%(variable)s_%(cmor_table)s_%(model)s_%(experiment)s_%(ensemble)s[_%(period_start)s-%(period_end)s].nc",
    "specs": [
        {
            "type": "collection",
            "namespace": "wcrp:tamip:variable",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "wcrp:tamip:cmor-table",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "wcrp:tamip:model",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "wcrp:tamip:experiment",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "wcrp:tamip:ensemble",
            "is_required": true
        },
        {
            "type": "regex",
            "expression": "^[0-9]*-[0-9]*$",
            "is_required": false
        },
        {
            "type": "regex",
            "expression": "^nc$",
            "is_required": true
        }
    ]
}