{
    "identifier_type": "filename",
    "scope": "ecmwf:c3s-cordex",
    "template": "%(variable)s_%(domain)s_%(driving_model)s_%(experiment)s_%(ensemble)s_%(rcm_model)s_%(rcm_version)s_%(time_frequency)s[_%(period_start)s-%(period_end)s].nc",
    "seperator": "_",
    "specs": [
        "ecmwf:c3s-cordex:variable",
        "ecmwf:c3s-cordex:domain",
        "ecmwf:c3s-cordex:driving_model",
        "ecmwf:c3s-cordex:experiment",
        "ecmwf:c3s-cordex:ensemble",
        "ecmwf:c3s-cordex:rcm_model",
        "ecmwf:c3s-cordex:rcm_version",
        "ecmwf:c3s-cordex:time_frequency",
        "regex:^[0-9]{4}-[0-9]{4}$",
        "regex:^nc$"
    ]
}