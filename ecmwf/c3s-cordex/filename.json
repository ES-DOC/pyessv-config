{
    "identifier_type": "filename",
    "scope": "ecmwf:c3s-cordex",
    "seperator": "_",
    "template": "%(variable)s_%(domain)s_%(driving_model)s_%(experiment)s_%(ensemble)s_%(rcm_model)s_%(rcm_version)s_%(time_frequency)s[_%(period_start)s-%(period_end)s].nc",
    "specs": [
        {
            "type": "collection",
            "namespace": "ecmwf:c3s-cordex:variable",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "ecmwf:c3s-cordex:domain",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "ecmwf:c3s-cordex:driving-model",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "ecmwf:c3s-cordex:experiment",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "ecmwf:c3s-cordex:ensemble",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "ecmwf:c3s-cordex:rcm-model",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "ecmwf:c3s-cordex:rcm-version",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "ecmwf:c3s-cordex:time-frequency",
            "is_required": true
        },
        {
            "type": "regex",
            "expression": "^[0-9]*-[0-9]*$",
            "is_required": false
        },
        {
            "type": "regex",
            "expression": "^nc$",
            "is_required": true
        }
    ]
}