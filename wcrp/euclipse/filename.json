{
    "identifier_type": "filename",
    "scope": "wcrp:euclipse",
    "seperator": "_",
    "template": "%(variable)s_%(cmor_table)s_%(model)s_%(experiment)s_%(ensemble)s[_%(period_start)s-%(period_end)s].nc",
    "specs": [
        {
            "type": "collection",
            "namespace": "wcrp:euclipse:variable",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "wcrp:euclipse:cmor-table",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "wcrp:euclipse:model",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "wcrp:euclipse:experiment",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "wcrp:euclipse:ensemble",
            "is_required": true
        },
        {
            "type": "regex",
            "expression": "^[0-9]{4}-[0-9]{4}$",
            "is_required": false
        },
        {
            "type": "regex",
            "expression": "^nc$",
            "is_required": true
        }
    ]
}