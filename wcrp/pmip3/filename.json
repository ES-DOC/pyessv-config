{
    "identifier_type": "filename",
    "scope": "wcrp:pmip3",
    "template": "%(variable)s_%(cmor_table)s_%(model)s_%(experiment)s_%(ensemble)s[_%(period_start)s-%(period_end)s].nc",
    "seperator": "_",
    "specs": [
        "wcrp:pmip3:variable",
        "wcrp:pmip3:cmor-table",
        "wcrp:pmip3:model",
        "wcrp:pmip3:experiment",
        "wcrp:pmip3:ensemble",
        "regex:^[0-9]{4}-[0-9]{4}$",
        "regex:^nc$"
    ]
}