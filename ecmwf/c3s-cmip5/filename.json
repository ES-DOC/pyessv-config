{
    "identifier_type": "filename",
    "scope": "ecmwf:c3s-cmip5",
    "template": "%(variable)s_%(cmor_table)s_%(model)s_%(experiment)s_%(ensemble)s[_%(period_start)s-%(period_end)s].nc",
    "seperator": "_",
    "specs": [
        "ecmwf:c3s-cmip5:variable",
        "ecmwf:c3s-cmip5:cmor_table",
        "ecmwf:c3s-cmip5:model",
        "ecmwf:c3s-cmip5:experiment",
        "ecmwf:c3s-cmip5:ensemble",
        "regex:^[0-9]{4}-[0-9]{4}$",
        "regex:^nc$"
    ]
}