{
    "identifier_type": "filename",
    "scope": "wcrp:primavera",
    "seperator": "_",
    "template": "%(variable)s_%(cmor_table)s_%(model)s_%(experiment)s_%(ensemble)s_%(grid_label)s_[%(period_start)s-%(period_end)s].nc",
    "specs": [
        {
            "type": "collection",
            "namespace": "wcrp:primavera:variable",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "wcrp:primavera:cmor-table",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "wcrp:primavera:model",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "wcrp:primavera:experiment",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "wcrp:primavera:ensemble",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "wcrp:primavera:grid-label",
            "is_required": true
        },
        {
            "type": "regex",
            "expression": "^[0-9]*-[0-9]*$",
            "is_required": false
        },
        {
            "type": "regex",
            "expression": "^nc$",
            "is_required": true
        }
    ]
}