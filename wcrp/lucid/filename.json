{
    "identifier_type": "filename",
    "scope": "wcrp:lucid",
    "template": "%(variable)s_%(cmor_table)s_%(model)s_%(experiment)s_%(ensemble)s[_%(period_start)s-%(period_end)s].nc",
    "seperator": "_",
    "specs": [
        "wcrp:lucid:variable",
        "wcrp:lucid:cmor-table",
        "wcrp:lucid:model",
        "wcrp:lucid:experiment",
        "wcrp:lucid:ensemble",
        "regex:^[0-9]{4}-[0-9]{4}$",
        "regex:^nc$"
    ]
}