{
    "identifier_type": "filename",
    "scope": "wcrp:lucid",
    "seperator": "_",
    "template": "%(variable)s_%(cmor_table)s_%(model)s_%(experiment)s_%(ensemble)s[_%(period_start)s-%(period_end)s].nc",
    "specs": [
        {
            "type": "collection",
            "namespace": "wcrp:lucid:variable",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "wcrp:lucid:cmor-table",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "wcrp:lucid:model",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "wcrp:lucid:experiment",
            "is_required": true
        },
        {
            "type": "collection",
            "namespace": "wcrp:lucid:ensemble",
            "is_required": true
        },
        {
            "type": "regex",
            "expression": "^[0-9]{4}-[0-9]{4}$",
            "is_required": false
        },
        {
            "type": "regex",
            "expression": "^nc$",
            "is_required": true
        }
    ]
}